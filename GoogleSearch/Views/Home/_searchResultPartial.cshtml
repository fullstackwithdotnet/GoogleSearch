@model  IEnumerable<GoogleSearch.Models.SearchResult>
<p id="searchresultsnumber">About 5 results (@ViewBag.Duration miliseconds) </p>

@foreach (var item in Model)
{
    <div class="searchresult">
        <a href="@item.Url">@item.Url</a> <button>▼</button>
        <h2>@item.Title</h2>
        <p>@item.Description</p>
    </div>

}